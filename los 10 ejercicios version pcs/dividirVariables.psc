// dividir 2 numeros enteros
Algoritmo dividirVariables
	Definir num1, num2, resultado Como Entero;
	Escribir 'Escribe el primer numero de la ecuacion: ';
	Leer num1;
	Escribir 'Escribe el segundo numero de la ecuacion: ';
	Leer num2;
	resultado <- (num1/num2);
	Escribir 'El resultado de la division es: ', resultado;
FinAlgoritmo
