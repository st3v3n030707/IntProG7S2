// antecesor y sucesor de 2 numeros enteros
Algoritmo AntecesorSuscesorVariables
	Definir antecesor, sucesor, num1 Como Entero;
	Escribir 'Escribe tu numero: ';
	Leer num1;
	antecesor <- (num1-1);
	sucesor <- (num1+1);
	Escribir 'Tu antecesor es: ', antecesor;
	Escribir 'Tu sucesor es: ', sucesor;
FinAlgoritmo
